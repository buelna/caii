  </head>
  <body>
    <table class="table">
      <thead>
        <tr>
          <th>Nombre</th>
          <th>Fecha de Inicio</th>
          <th>Fecha de Terminaci√≥n</th>
          <th>Estatus</th>
          <th>Entidad Financiadora</th>
          <th>Monto</th>
          
        </tr>
      </thead>
      <tbody id="myTable">
          {% for entity in entities %}   
            <tr>
                <td>{{ entity.nombre }}</td>
                <td>{{ entity.fechaInicio|date('d-m-Y') }}</td>
                <td>{{ entity.fechaFinal|date('d-m-Y') }}</td>
                <td>{% if entity.status==1 %}
                      Activo
                    {% else %}
                      Inactivo
                    {% endif %}</td>
                <td>{{ entity.idEntidad }} </td>
                <td>{{ entity.montoFinanciero|number_format(2, '.', ',') }}</td>
                
            </tr>

          {% endfor %}

      </tbody>
    </table>
  </body>
